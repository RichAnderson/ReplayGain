<Window x:Class="ReplayGain.Help"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Help" Height="300" Width="300">
    <Window.Background>
        <LinearGradientBrush EndPoint="0.5,1.5" MappingMode="RelativeToBoundingBox" StartPoint="0.5,0">
            <GradientStop Color="#FF4079A8" Offset="1"/>
            <GradientStop Color="White"/>
        </LinearGradientBrush>
    </Window.Background>
    <Grid>
        <TextBox Name="HelpDialog" HorizontalAlignment="Left" Height="210" Width="250" Margin="19,35,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Background="#7FFFFFFF"/>
    </Grid>
</Window>
